{
    "name": "lawaxi/level-ranks",
    "description": "Add a level/experience bar to your flarum Forum.",
    "autoload": {
        "psr-4": {
            "Lawaxi\\LevelRanks\\": "src/"
        }
    },
    "type": "flarum-extension",
    "license": "MIT",
    "authors": [
        {
            "name": "Lawaxi",
            "email": "2330234142@qq.com"
        }
    ],
    "support": {
        "issues": "https://github.com/imorland/level-ranks/issues",
        "source": "https://github.com/imorland/level-ranks",
        "forum": "https://discuss.flarum.org/d/27869"
    },
    "require": {
        "flarum/core": "^1.0.2"
    },
    "replace": {
        "reflar/level-ranks": "*"
    },
    "extra": {
        "flarum-extension": {
            "title": "Level Ranks",
            "category": "feature",
            "icon": {
                "name": "fas fa-percent",
                "backgroundColor": "#2E8B57",
                "color": "#fff"
            }
        },
        "flagrow": {
            "discuss": "https://discuss.flarum.org/d/27869"
        }
    }
}
